<h2>Shelter Index</h2>
<% @shelters.each do |shelter| %>
  <h3><a href="/shelters/<%= shelter.id %>"><%= shelter.name %></h3>
  <%= link_to 'Update Shelter', "/shelters/#{shelter.id}/edit" %>
  <br><%= link_to 'Delete Shelter', "/shelters/#{shelter.id}", method: :delete %>
<% end %>
<h4><%= link_to 'New Shelter', "/shelters/new" %></h4>
