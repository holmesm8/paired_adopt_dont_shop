h1>New Review</h1>
  
<%= form_tag "/shelters/#{@shelter_id}/reviews", method: :post do %>
  <br><%= label_tag :title %>
  <br><%= text_field_tag :title %>
  <br><%= label_tag :rating %>
  <br><%= number_field_tag :rating %>
  <br><%= label_tag :content %>
  <br><%= text_field_tag :content %>
  <br><%= label_tag :"image (optional)" %>
  <br><%= text_field_tag :image %>
  <br>
  <%= submit_tag 'Create Pet'%>
<% end %>
